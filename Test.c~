#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#define N 3
/* fonction pour afficher la grille modifier */
void grille(char mat[N][N]){
	int i,j;
	for(i=0;i<N;i++){
		for(j=0;j<N;j++){
			mat[i][j]='u';
			printf("%c ",mat[i][j]);
		}
	}
}
int main(){
	
	char mat[N][N];
	int x,y;
	int i,j;
	grille(mat);

	
}
	
 